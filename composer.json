{
    "name": "district5/cache-lib",
    "description": "Cache lib is a caching library for PHP",
    "keywords": ["cache"],
    "homepage": "https://github.com/district-5/cache-lib",
    "license": "MIT",
    "authors": [
        {
            "name": "District5",
            "email": "hello@district5.co.uk"
        }
    ],
    "suggest": {
        "ext-apc": "To use the apc extension",
        "ext-apcu": "To use the apcu extension",
        "ext-memcache": "To use the Memcache adapter",
        "ext-memcached": "To use the Memcached adapter"
    },
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": { "District5\\": "src/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload-dev": {
        "psr-4": { "District5Tests\\CacheLibTests\\": "tests/CacheLibTests" }
    }
}
